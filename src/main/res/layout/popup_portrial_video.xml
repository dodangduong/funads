<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    xmlns:blurkit="http://schemas.android.com/tools"
    android:id="@+id/framePoup"
    android:background="@drawable/backgroundpopupportrailvideo"
    android:padding="5dp"
    android:layout_height="wrap_content">
    <funs.adds.android.sdk.component.view.FunAdsBannerView
        android:id="@+id/funads_banner_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <com.google.android.exoplayer2.ui.PlayerView
        android:layout_marginTop="90dp"
        android:id="@+id/video_view"
        android:background="@drawable/background_icon"
        android:layout_width="wrap_content"
        android:layout_marginRight="12dp"
        android:layout_marginLeft="12dp"
        android:layout_height="177dp">
    </com.google.android.exoplayer2.ui.PlayerView>
</FrameLayout>